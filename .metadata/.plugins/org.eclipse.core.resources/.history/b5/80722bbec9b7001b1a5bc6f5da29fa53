<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="demo">
	<insert id="insertDev">
		insert into
			dev
		values(
			seq_dev_no.nextval,
			#{name},
			#{career},
			#{email},
			#{gender},
			#{lang}
		)
	
	</insert>
	
	<select id="selectDevList" resultMap="devVoMap">
		select
			*
		from
			dev
		order by
			no desc
	</select>
  
  
</mapper>